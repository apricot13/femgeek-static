---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
layout: layouts/home.njk
eleventyComputed:
  title: Tagged “{{ tag }}”
permalink: /tags/{{ tag }}/
---
<h2>Tagged “{{ tag }}”</h2>

{% set postslist = collections[ tag ] %}
{% include "postslist.njk" %}

<br />
<br />
<p class="text-center">View <a href="{{ '/tags/' | url }}">all tags</a>.</p>
